<% semantic_form_for @search do |f| %>
  <%= f.inputs :name_like, :address_like, :dl_like, :tin_like, :franchisee_login_like %>
  <%= f.submit 'Search' %>
<% end %>

<% table_for @franchises do |t| %>
  <%= t.data :actions => :all %>
<% end %>

<%= link_to 'New franchise', new_franchise_path, :id => 'add_new', :class => 'ui-icon ui-icon-plusthick' %>
