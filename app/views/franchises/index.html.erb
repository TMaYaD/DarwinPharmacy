<h1>Listing franchises</h1>

<table>
  <tr>
    <th>Franchisee</th>
    <th>Name</th>
    <th>Address</th>
  </tr>

<% @franchises.each do |franchise| %>
  <tr>
    <td><%=h franchise.franchisee.name %></td>
    <td><%=h franchise.name %></td>
    <td><%=h franchise.address %></td>
    <td><%= link_to 'Show', franchise %></td>
    <td><%= link_to 'Edit', edit_franchise_path(franchise) %></td>
    <td><%= link_to 'Destroy', franchise, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New franchise', new_franchise_path %>
