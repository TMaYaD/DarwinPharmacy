<h1>New stock_inventory</h1>

<% form_for(@stock_inventory) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :franchise %><br />
    <%= f.text_field :franchise %>
  </p>
  <p>
    <%= f.label :product_batch %><br />
    <%= f.text_field :product_batch %>
  </p>
  <p>
    <%= f.label :quantity %><br />
    <%= f.text_field :quantity %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', stock_inventories_path %>